  "bugs": {
    "url": "https://github.com/dfirebaugh/memberserver/issues"
  },
  "proxy": "http://localhost:3000",
  "homepage": "https://github.com/dfirebaugh/memberserver#readme",
  "dependencies": {
    "@material/mwc-button": "^0.20.0",
    "@material/mwc-fab": "^0.20.0",
    "@material/mwc-icon": "^0.20.0",
    "@material/mwc-icon-button": "^0.20.0",
    "@material/mwc-menu": "^0.20.0",
    "@material/mwc-snackbar": "^0.20.0",
    "@material/mwc-textfield": "^0.20.0",
    "@material/mwc-top-app-bar-fixed": "^0.20.0",
    "lit-element": "^2.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.3.3",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.1",
    "jest": "^26.4.2",
    "es-dev-server": "^1.52.0",
    "typescript": "^4.0.2"
  },
  "type": "module"
}
